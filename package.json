{
  "dependencies": {
    "bootstrap-icons": "^1.11.3"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "sass": "^1.85.1"
  },
  "scripts": {
    "build:css": "sass src/scss/main.scss:styles/main.css --style compressed",
    "copy:fonts": "cpx node_modules/bootstrap-icons/font/fonts/* styles/fonts",
    "build": "yarn copy:fonts && yarn build:css"
  }
}
